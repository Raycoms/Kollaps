<?xml version="1.0" encoding="UTF-8" ?>
<experiment>
	<services>
		<service name="client" image="warpenguin.no-ip.org/udpclient:4.0" command="['server']" replicas="2" share="true"/>
		<service name="server" image="warpenguin.no-ip.org/server:4.0" replicas="2" share="false"/>
	</services>
	<bridges>
                <bridge name="s1"/>
                <bridge name="s2"/>
	</bridges>
        <links>
                <link origin="s1" dest="client" latency="5"  drop="0" upload="10Mbps" download="10Mbps" network="test1"/>
                <link origin="server" dest="s2" latency="5"  drop="0" upload="100Mbps" download="100Mbps" network="test1"/>
                <link origin="s1" dest="s2" latency="10"  drop="0" upload="50Mbps" download="50Mbps" network="test1"/>
        </links>
</experiment>
